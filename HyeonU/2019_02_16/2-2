#include<stdio.h>

int main(){
	int num[100];
	int i = 0,max = 0, min = 0;
	for(i = 0; i < 100; i++){
		if(num[i] > num [i+1]){
			max = num[i];
		}
		else{
			max = num[i+1];
		}
	}
	
	for(i = 0; i < 100; i++){
		if(num[i] < num [i+1]){
			min = num[i];
		}
		else{
			min = num[i+1];
		}
	}
	
	printf("\n최대값 : %d\n",max);
	printf("\n최소값 : %d\n",min);
	return 0;
}
