#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(){
	int a[25][2];
	int r[25];
	int i;
	srand((unsigned)time(NULL));
	for(i = 0; i < 25; i++){
		a[i][1] = rand()%101;
	}
	int j;
	for(i = 0; i < 25; i++){
		r[i] = 0;
	}
	for(i = 0; i < 25; i++){
		for(j = 0;j<25;j++){
			if(a[i][1]<=a[j][1]){
				r[i] += 1;
			}
		}
		printf("\t%d\t%d\n",a[i][1],r[i]);
	}
	return 0;
}
