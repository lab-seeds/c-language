#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(){
	int a[350][2];
	int i;
	srand((unsigned)time(NULL));
	for(i = 0; i < 350; i++){
		a[i][0] = rand()%101;
	}
	int j;
	for(i = 0; i < 350; i++){
		a[i][1] = 0;
	}
	for(i = 0; i < 350; i++){
		for(j = 0;j < 350;j++){
			if(a[i][0]<=a[j][0]){
				a[i][1] += 1;
			}
		}
		printf("\t%d\t%d\n",a[i][0],a[i][1]);
	}
	return 0;
}
