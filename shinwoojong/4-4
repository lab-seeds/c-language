#include <stdio.h>
#include <time.h>
#include <stdlib.h>

int main()
{
	int i,j;
	int a[3] = {0};
	int key;
	srand((unsigned)time(NULL));
	
	for(i=0; i<3; i++)
	{
		a[i] = rand()%100;
	}
	for(i=0; i<3; i++)
	{
		printf("%d\n",a[i]);
	}

	for(i=1; i<3; i++)
	{
		key = a[i];
		
		for(j=i-1; j<=0; j--)
		{
			if(key < a[j])
			{
				a[j+1] = a[j];
				
			}
			else
			{
				a[j+1] = key;
				break;
			}
		}
	}
	for(i=0; i<3; i++)
	{
		printf("%d\n",a[i]);
	}
	
	
	
	return 0;
	
}
